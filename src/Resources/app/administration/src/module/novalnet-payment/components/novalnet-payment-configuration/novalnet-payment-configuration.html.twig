{% block novalnet_payment_content_card_channel_config_payment %}
<sw-card-view style="position: relative;">
    <sw-card class="sw-card--grid">
        {% block novalnet_payment_content_card_channel_config_payment_card_sepa_container %}
            <sw-container>
                <sw-collapse>

                    <div slot="header">
                        <div class="novalnet_payment_card_title">
                                {{ $tc('novalnet-payment.settingForm.paymentSettings.sepa.cardTitle') }}
                        </div>
                        <div class="novalnet_payment_card_head_down_bar">
                            <sw-icon name="regular-chevron-down" small="true">
                            </sw-icon>
                        </div>
                        </div>

                    <div slot="content">
                        {% block novalnet_payment_content_card_channel_config_payment_card_sepa_container_settings %}
                            <div v-if="actualConfigData" class="novalnet_payment-settings-credentials">
                                {% block novalnet_payment_content_card_channel_config_payment_card_container_settings_sepa %}
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaTestMode']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaTestMode']"
                                                        :customInheritationCheckFunction="checkBoolFieldInheritance">
                                        <template #content="props">
                                            <sw-switch-field name="NovalnetPayment.settings.sepaTestMode"
                                                           :mapInheritance="props"
                                                           :disabled="props.isInherited"
                                                           :label="$tc('novalnet-payment.settingForm.paymentSettings.testmode.label')"
                                                           :helpText="$tc('novalnet-payment.settingForm.paymentSettings.testmode.tooltipText')"
                                                           :value="props.currentValue"
                                                           @change="props.updateCurrentValue">
                                            </sw-switch-field>
                                        </template>
                                    </sw-inherit-wrapper>
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaOneclick']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaOneclick']"
                                                        :customInheritationCheckFunction="checkBoolFieldInheritance">
                                        <template #content="props">
                                            <sw-switch-field name="NovalnetPayment.settings.sepaOneclick"
                                                           :mapInheritance="props"
                                                           :disabled="props.isInherited"
                                                           :label="$tc('novalnet-payment.settingForm.paymentSettings.oneclick.label')"
                                                           :helpText="$tc('novalnet-payment.settingForm.paymentSettings.oneclick.tooltipText')"
                                                           :value="props.currentValue"
                                                           @change="props.updateCurrentValue">
                                            </sw-switch-field>
                                        </template>
                                    </sw-inherit-wrapper>
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaDueDate']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaDueDate']">
                                        <template #content="props">
                                            <sw-number-field name="NovalnetPayment.settings.sepaDueDate"
                                                             numberType="int"
                                                             :min="2"
                                                             :max="14"
                                                             :mapInheritance="props"
                                                             :disabled="props.isInherited"
                                                             :label="$tc('novalnet-payment.settingForm.paymentSettings.sepa.dueDate.label')"
                                                             :helpText="$tc('novalnet-payment.settingForm.paymentSettings.sepa.dueDate.tooltipText')"
                                                             :value="props.currentValue"
                                                             @change="props.updateCurrentValue">
                                            </sw-number-field>
                                        </template>
                                    </sw-inherit-wrapper>
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaOnHold']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaOnHold']">
                                        <template #content="props">
                                            <sw-single-select name="NovalnetPayment.settings.sepaOnHold"
                                                              labelProperty="name"
                                                              valueProperty="id"
                                                              :options="onHoldZeroOptions"
                                                              :label="$tc('novalnet-payment.settingForm.paymentSettings.onHold.label')"
                                                              :helpText="$tc('novalnet-payment.settingForm.paymentSettings.onHold.tooltipText')"
                                                              :isInherited="props.isInherited"
                                                              :value="props.currentValue == null ? selected : props.currentValue"
                                                              @change="props.updateCurrentValue">
                                            </sw-single-select>
                                        </template>
                                    </sw-inherit-wrapper>
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaOnHoldAmount']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaOnHoldAmount']">
                                        <template #content="props">
                                            <sw-number-field name="NovalnetPayment.settings.sepaOnHoldAmount"
                                                             numberType="int"
                                                             :mapInheritance="props"
                                                             :disabled="props.isInherited"
                                                             :label="$tc('novalnet-payment.settingForm.paymentSettings.onHold.amountLabel')"
                                                             :helpText="$tc('novalnet-payment.settingForm.paymentSettings.onHold.amountTooltip')"
                                                             :value="props.currentValue == 0 ? null : props.currentValue"
                                                           @change="props.updateCurrentValue">
                                            </sw-number-field>
                                        </template>
                                    </sw-inherit-wrapper>
                                    <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.sepaNotify']"
                                                        :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.sepaNotify']"
                                                        :customInheritationCheckFunction="checkTextFieldInheritance">
                                        <template #content="props">
                                            <sw-text-field name="NovalnetPayment.settings.sepaNotify"
                                                           :mapInheritance="props"  :label="$tc('novalnet-payment.settingForm.paymentSettings.notification.label')"
                                                           :helpText="$tc('novalnet-payment.settingForm.paymentSettings.notification.tooltipText')"
                                                           :disabled="props.isInherited"
                                                           :value="props.currentValue"
                                                           @change="props.updateCurrentValue">
                                            </sw-text-field>
                                        </template>
                                    </sw-inherit-wrapper>
                                {% endblock %}
                            </div>
                        {% endblock %}
                    </div>
                </sw-collapse>
            </sw-container>
        {% endblock %}
    </sw-card>

